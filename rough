REM INSERTING into DF_QUERY
SET DEFINE OFF;
Insert into DF_QUERY (NAME,QUERY) values ('df_inp','select  DF_INP_PK, DF_TYPE,  DF_NAME,  DF_SYSTEM,  DF_INFO,  CONNECTION_INFO,  FILE_DELIM,  FIELD_QUOTE, SKIP_BLANK_LINES, TRIM_SPACES, REPLACE_BLANKS_BY, LINES_TO_LOAD, encoding, CONTAINS_HEADER, CONTAINS_TRAILER, LINES_TO_SKIP, POLL_INTERVAL_MINUTES, POLL_COUNT_NUMBER  from  DF_INP where  CURRENT_IND = ''Y'' AND DF_INP_PK  = :inp_bind_value');
Insert into DF_QUERY (NAME,QUERY) values ('df_validation','SELECT  VALIDATION_NAME, VALIDATION_CMD, DF_VALIDATION_PARAMETER, DF_VALIDATION_CHECK, FILE_DELIM, FIELD_QUOTE, SKIP_BLANK_LINES, TRIM_SPACES, REPLACE_BLANKS_BY, LINES_TO_LOAD, ENCODING, CONTAINS_HEADER, CONTAINS_TRAILER, LINES_TO_SKIP  FROM  DF_INP_VALIDATION A, DF_VALIDATION B, DF_INP C WHERE A.DF_VALIDATION_FK = B.DF_VALIDATION_PK AND A.DF_INP_FK = C.DF_INP_PK AND A.CURRENT_IND = ''Y'' AND  B.CURRENT_IND = ''Y'' AND C.CURRENT_IND = ''Y'' AND A.DF_INP_FK = :inp_bind_value');
Insert into DF_QUERY (NAME,QUERY) values ('BATCH_JRNL_ID','1');
Insert into DF_QUERY (NAME,QUERY) values ('VALID_ROWS','select C1, C2 from NAV_SPECTRA_STG where 1 = :inp_bind_value');
